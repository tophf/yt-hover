{
  "name": "YouTubeâ„¢ on Hover Preview",
  "short_name": "ihvyoutube",
  "description": "View YouTube while hovering mouse over a YouTube link on any website",
  "author": "tophf + Jeremy Schomery",
  "version": "1.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "61",
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "optional_permissions": [
    "history"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "bg/bg.js"
    ]
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "exclude_matches": ["https://www.youtube.com/embed/*"],
    "js": ["content/hover.js"],
    "all_frames": true,
    "match_about_blank": true
  }],
  "icons": {
    "16": "ui/icon/16.png",
    "32": "ui/icon/32.png",
    "48": "ui/icon/48.png",
    "64": "ui/icon/64.png",
    "128": "ui/icon/128.png",
    "256": "ui/icon/256.png"
  },
  "options_ui": {
    "page": "ui/options.html",
    "chrome_style": false
  },
  "key": ""
}
